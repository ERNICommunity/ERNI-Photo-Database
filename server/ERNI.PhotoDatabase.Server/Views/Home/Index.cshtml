@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers 
@using ERNI.PhotoDatabase.Server.Controllers
@model ERNI.PhotoDatabase.Server.Controllers.SearchResult[]
@{ 
    Layout = "_Layout";
}
    @if (Model != null)
    {
	    foreach (var sr in Model)
	    {
		    <img src="/api/image/@Html.UrlEncoder.Encode(sr.ImagePath)/download"/>
	    }
    }